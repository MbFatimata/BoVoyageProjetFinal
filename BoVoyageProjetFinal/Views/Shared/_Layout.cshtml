<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>BoVoyage | @ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
</head>

<body>
    @*<img src="C:\Users\Adminl\Desktop\LOGO.png" height="100" width="100" align="middle"/>*@
    <div class="container">
        <nav class="navbar navbar-expand-lg navbar-dark bg-warning">
            <a class="navbar-brand" href="~/">Accueil</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link" href=" @Url.Action("About","Home") ">A propos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href=" @Url.Action("Index","Travels") ">Voyages</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href=" @Url.Action("Create","ContactMessages") ">Nous contacter</a>
                    </li>

                    @*
                        <li class="nav-item active">
                            <a class="nav-link" href="@Url.Action("Index", "Dashboard", new { area = "BackOffice" })">Back Office<span class="sr-only">(current)</span></a>
                        </li>

                    *@
                </ul>
                <span class="navbar-text">
                    <a href="@Url.Action("Create", "Clients")" class="btn btn-warning btn-sm">Créer un compte</a>
                </span>
                <span> </span>
                <span class="navbar-text">
                    <a href="@Url.Action("Login", "AuthenticationClient")" class="btn btn-warning btn-sm"><i class="fa fa-sign-in"></i> Se connecter</a>
                </span>
            </div>
        </nav>
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="https://back-promocam.orchestra-platform.com/admin/TS/fckUserFiles/Image/PHOTOS_DIRECTOURS/Iles/Maldives/Cocoon/vignette.jpg" alt="First slide" width="1100" height="500">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://www.readytogo.fr/files/fichepays/undefined_1492790487_min9.jpg" alt="Second slide" width="1100" height="500">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://www.google.com/url?sa=i&rct=j&q=&esrc=s&source=images&cd=&cad=rja&uact=8&ved=2ahUKEwiEk474wb_cAhXS4IUKHSgaC5UQjRx6BAgBEAU&url=https%3A%2F%2Fwww.leconomistemaghrebin.com%2F2017%2F06%2F08%2Fvisas-gratuits-visiter-dubai%2F&psig=AOvVaw2Whq0kO1NoezBcuDsFBgVD&ust=1532788937081339" alt="Third slide" width="1100" height="500">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
        @RenderBody()
        <hr />

        <footer class="bg-warning text-white">
            &copy; 2018 BoVoyage
            @*@Html.Partial("_TopFive", *@
            @*@@Html.Action("TopFive", "Shared")*@
        </footer>

    </div>
    <script src="~/Scripts/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    @RenderSection("SectionScripts", false)
</body>
</html>