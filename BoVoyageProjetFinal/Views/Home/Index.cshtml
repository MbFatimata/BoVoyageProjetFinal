@model IEnumerable<BoVoyageProjetFinal.Models.Travel>
@{
    ViewBag.Title = "Index";
}
<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="https://back-promocam.orchestra-platform.com/admin/TS/fckUserFiles/Image/PHOTOS_DIRECTOURS/Iles/Maldives/Cocoon/vignette.jpg" alt="First slide" width="1100" height="500">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="https://www.readytogo.fr/files/fichepays/undefined_1492790487_min9.jpg" alt="Second slide" width="1100" height="500">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="http://www.beyaztur.com/uploads/slider/alternatif-yurtdisi-turlar-114228.jpg" alt="Third slide" width="1100" height="500">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<h2>Accueil</h2>

<h3>Top 5 des voyages les moins couteux</h3>
@Html.Action("TopFiveAllInclusivePrice", "Shared")

<h3>Top 5 des voyages dont la date de départ est la plus proche</h3>
@Html.Action("TopFiveTravelsDepartureDate", "Shared")

<h3>Top 5 des voyages dont les pays sont les plus présents</h3>
@Html.Action("TopFiveTravelsCountry", "Shared")
